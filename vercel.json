{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "github": {
    "enabled": true
  },
  "env": {
    "NEXT_PUBLIC_ENVIRONMENT": "staging",
    "NEXT_PUBLIC_TEST_MODE": "true"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}